using System;
using System.Linq.Expressions;
using ConsoleApp4.Enums;
using ConsoleApp4.Temprature;

namespace ConsoleApp4
{
    internal class Program
    {
        static void Main(string[] args)
        {
            var celsius = new Temprature.Celsius { Degree = 25 };
            Temprature.Kelvin kelvin = celsius.ToKelvin();
            Console.WriteLine($"{celsius.Degree} °C is equal to {kelvin.Degree} K");
        }
    }
}
